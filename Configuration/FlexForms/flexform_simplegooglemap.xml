<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
		<langChildren>0</langChildren>
	</meta>
	<sheets>
		<sDEF>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:cbgooglemaps/Resources/Private/Language/locallang.xml:label.sheetLocation</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>					
					<settings.cbgmIncludeJs>
						<TCEforms>
							<label></label>
							<config>
								<type>user</type>
								<userFunc>tx_cbgooglemaps_includeBeJavascript->includeCbGoogleMapsJavascript</userFunc>
							</config>
						</TCEforms>
					</settings.cbgmIncludeJs>	
					<settings.cbgmStreet>
						<TCEforms>
							<label>LLL:EXT:cbgooglemaps/Resources/Private/Language/locallang.xml:label.street</label>
							<config>
								<type>input</type>
								<size>40</size>
								<eval>trim</eval>
							</config>
						</TCEforms>
					</settings.cbgmStreet>
					<settings.cbgmZip>
						<TCEforms>
							<label>LLL:EXT:cbgooglemaps/Resources/Private/Language/locallang.xml:label.zip</label>
							<config>
								<type>input</type>
								<size>10</size>
								<eval>trim</eval>
							</config>
						</TCEforms>
					</settings.cbgmZip>
					<settings.cbgmCity>
						<TCEforms>
							<label>LLL:EXT:cbgooglemaps/Resources/Private/Language/locallang.xml:label.city</label>
							<config>
								<type>input</type>
								<size>40</size>
								<eval>trim</eval>
							</config>
						</TCEforms>
					</settings.cbgmCity>
					<settings.cbgmCountry>
						<TCEforms>
							<label>LLL:EXT:cbgooglemaps/Resources/Private/Language/locallang.xml:label.country</label>
							<config>
								<type>input</type>
								<size>40</size>
								<eval>trim</eval>
							</config>
						</TCEforms>
					</settings.cbgmCountry>					
					<cbgmButtonGeocoding>
						<TCEforms>
							<label></label>
							<config>
								<type>user</type>
								<userFunc>tx_cbgooglemaps_addFlexformFields->addGeocodingButton</userFunc>
							</config>
						</TCEforms>
					</cbgmButtonGeocoding>					
					<settings.cbgmLatitude>
						<TCEforms>
							<label>LLL:EXT:cbgooglemaps/Resources/Private/Language/locallang.xml:label.latitude</label>
							<config>
								<type>input</type>
								<size>25</size>
								<eval>trim</eval>
							</config>
						</TCEforms>
					</settings.cbgmLatitude>
					<settings.cbgmLongitude>
						<TCEforms>
							<label>LLL:EXT:cbgooglemaps/Resources/Private/Language/locallang.xml:label.longitude</label>
							<config>
								<type>input</type>
								<size>25</size>
								<eval>trim</eval>
							</config>
						</TCEforms>
					</settings.cbgmLongitude>
				</el>
			</ROOT>
		</sDEF>
		<s_displayproperties>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:cbgooglemaps/Resources/Private/Language/locallang.xml:label.sheetDisplay</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>	
					<settings.cbgmDescription>
						<TCEforms>
							<label>LLL:EXT:cbgooglemaps/Resources/Private/Language/locallang.xml:label.description</label>
							<config>
								<type>text</type>
								<cols>50</cols>
								<rows>10</rows>
								<eval>trim</eval>
							</config>
						</TCEforms>
					</settings.cbgmDescription>
					<settings.cbgmMapWidth>
						<TCEforms>
							<label>LLL:EXT:cbgooglemaps/Resources/Private/Language/locallang.xml:label.mapWidth</label>
							<config>
								<type>input</type>
								<size>4</size>
								<eval>trim</eval>
							</config>
						</TCEforms>
					</settings.cbgmMapWidth>	
					<settings.cbgmMapHeight>
						<TCEforms>
							<label>LLL:EXT:cbgooglemaps/Resources/Private/Language/locallang.xml:label.mapHeight</label>
							<config>
								<type>input</type>
								<size>4</size>
								<eval>trim</eval>
							</config>
						</TCEforms>
					</settings.cbgmMapHeight>		
					
					<settings.cbgmMapType>
						<TCEforms>
							<label>LLL:EXT:cbgooglemaps/Resources/Private/Language/locallang.xml:label.mapType</label>
							<config>
								<type>select</type>
								<items type="array">
									<numIndex index="0" type="array">
										<numIndex index="0"></numIndex>
										<numIndex index="1"></numIndex>
									</numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">LLL:EXT:cbgooglemaps/Resources/Private/Language/locallang.xml:label.mapTypeRoadmap</numIndex>
										<numIndex index="1">ROADMAP</numIndex>
									</numIndex>
									<numIndex index="2" type="array">
										<numIndex index="0">LLL:EXT:cbgooglemaps/Resources/Private/Language/locallang.xml:label.mapTypeSatellite</numIndex>
										<numIndex index="1">SATELLITE</numIndex>
									</numIndex>
									<numIndex index="3" type="array">
										<numIndex index="0">LLL:EXT:cbgooglemaps/Resources/Private/Language/locallang.xml:label.mapTypeHybrid</numIndex>
										<numIndex index="1">HYBRID</numIndex>
									</numIndex>
									<numIndex index="4" type="array">
										<numIndex index="0">LLL:EXT:cbgooglemaps/Resources/Private/Language/locallang.xml:label.mapTypeTerrain</numIndex>
										<numIndex index="1">TERRAIN</numIndex>
									</numIndex>
								</items>
							</config>
						</TCEforms>
					</settings.cbgmMapType>
					<settings.cbgmScaleLevel>
						<TCEforms>
							<label>LLL:EXT:cbgooglemaps/Resources/Private/Language/locallang.xml:label.scaleLevel</label>
							<config>
								<type>input</type>
								<size>3</size>
								<eval>trim</eval>
							</config>
						</TCEforms>
					</settings.cbgmScaleLevel>					
					<settings.cbgmNavigationControl>
						<TCEforms>
							<label>LLL:EXT:cbgooglemaps/Resources/Private/Language/locallang.xml:label.navigationControl</label>
							<config>
								<type>select</type>
								<items type="array">
									<numIndex index="0" type="array">
										<numIndex index="0"></numIndex>
										<numIndex index="1"></numIndex>
									</numIndex>									
									<numIndex index="1" type="array">
										<numIndex index="0">LLL:EXT:cbgooglemaps/Resources/Private/Language/locallang.xml:label.navigationControlDefault</numIndex>
										<numIndex index="1">DEFAULT</numIndex>
									</numIndex>
									<numIndex index="2" type="array">
										<numIndex index="0">LLL:EXT:cbgooglemaps/Resources/Private/Language/locallang.xml:label.navigationControlSmall</numIndex>
										<numIndex index="1">SMALL</numIndex>
									</numIndex>
									<numIndex index="3" type="array">
										<numIndex index="0">LLL:EXT:cbgooglemaps/Resources/Private/Language/locallang.xml:label.navigationControlZoomPan</numIndex>
										<numIndex index="1">ZOOM_PAN</numIndex>
									</numIndex>
									<numIndex index="4" type="array">
										<numIndex index="0">LLL:EXT:cbgooglemaps/Resources/Private/Language/locallang.xml:label.navigationControlAndroid</numIndex>
										<numIndex index="1">ANDROID</numIndex>
									</numIndex>
								</items>
							</config>
						</TCEforms>
					</settings.cbgmNavigationControl>
							
					<cbgmButtonGeocoding>
						<TCEforms>
							<label></label>
							<config>
								<type>user</type>
								<userFunc>tx_cbgooglemaps_addFlexformFields->addPreviewButton</userFunc>
							</config>
						</TCEforms>
					</cbgmButtonGeocoding>				
				</el>				
			</ROOT>
		</s_displayproperties>
	</sheets>
</T3DataStructure>